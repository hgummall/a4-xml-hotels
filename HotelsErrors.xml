<?xml version="1.0" encoding="UTF-8"?>
<!-- Faulty on purpose: contains 5 independent errors -->
<Hotel xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:noNamespaceSchemaLocation="Hotels.xsd">
  <!-- (1) Root element name is Hotel instead of Hotels -->

  <Hotel Rating="4.2">
    <Name>Westin</Name>
    <!-- (3) Remove a required Phone occurrence on one hotel: here we delete all Phone elements -->
    <Address>
      <Number>11</Number>
      <Street>E 7th St</Street>
      <City>Tempe</City>
      <State>AZ</State>
      <Zip>85281</Zip>
      <NearestAirport>Sky Harbor</NearestAirport>
    </Address>
  </Hotel>

  <!-- (2) Missing required element example: omit <City> inside Address -->
  <Hotel>
    <Name>Courtyard Tempe Downtown</Name>
    <Phone>480-966-2800</Phone>
    <Address>
      <Number>601</Number>
      <Street>S Ash Ave</Street>
      <!-- City is missing (required by XSD) -->
      <State>AZ</State>
      <Zip>85281</Zip>
      <NearestAirport>Sky Harbor</NearestAirport>
    </Address>
  </Hotel>

  <!-- (4) An Address element has no closing tag -->
  <Hotel>
    <Name>Tempe Mission Palms</Name>
    <Phone>480-894-1400</Phone>
    <Address>
      <Number>60</Number>
      <Street>E 5th St</Street>
      <City>Tempe</City>
      <State>AZ</State>
      <!-- MISSING </Address> closing tag on purpose -->
  </Hotel>

  <!-- (5) Two names for one hotel -->
  <Hotel>
    <Name>Graduate Tempe</Name>
    <Name>Duplicate Name</Name>
    <Phone>480-967-9431</Phone>
    <Address>
      <Number>225</Number>
      <Street>E Apache Blvd</Street>
      <City>Tempe</City>
      <State>AZ</State>
      <Zip>85281</Zip>
      <NearestAirport>Sky Harbor</NearestAirport>
    </Address>
  </Hotel>
</Hotel>
